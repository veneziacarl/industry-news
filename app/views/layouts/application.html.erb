<!DOCTYPE html>
<html>
<head>
  <title>Aggregator</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "layouts/header" %>

  <div class="messages">
    <% flash.each do |type, message| %>
      <%= message %>
    <% end %>
  </div>

<%= yield %>

</body>
</html>
